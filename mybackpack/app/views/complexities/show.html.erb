<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @complexity.title %>
</p>

<p>
  <strong>Description:</strong>
  <%= @complexity.description %>
</p>

<p>
  <strong>Value:</strong>
  <%= @complexity.value %>
</p>

<p>Chains: </p>
<div>
  <p>
    <% @complexity.chain.each do |chain| %>
      <%= link_to chain.title, chain_url %> |
    <% end %>
  </p>
</div>

<p>Routes: </p>
<div>
  <p>
    <% @complexity.route.each do |route| %>
      <%= link_to route.title, route_url %> |
    <% end %>
  </p>
</div>

<%= link_to 'Edit', edit_complexity_path(@complexity) %> |
<%= link_to 'Back', complexities_path %>
